{
  "name": "rscute",
  "version": "0.2.5",
  "description": "Rust Execute (rscute): Run TypeScript (ESMãƒ»CJS) in memory with SWC",
  "keywords": [
    "cli",
    "runtime",
    "node",
    "cjs",
    "commonjs",
    "esm",
    "typescript",
    "typescript runner"
  ],
  "author": "Refirst",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/refirst11/rscute.git"
  },
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./execute": {
      "import": "./dist/execute.js",
      "require": "./dist/execute.js",
      "types": "./types/execute.d.ts"
    }
  },
  "main": "dist/index.js",
  "bin": {
    "rscute": "./dist/cli.js"
  },
  "files": [
    "dist/",
    "types/"
  ],
  "scripts": {
    "build": "rimraf dist types && npx tsc && pnpm minify && pnpm minify && pnpm bundle",
    "minify": "terser dist/execute.js -c -o dist/execute.js",
    "bundle": "pnpm cpy 'bundle/**' 'dist'",
    "test": "node dist/cli.js test/test.tsx"
  },
  "dependencies": {
    "@swc/core": "^1.11.22"
  },
  "devDependencies": {
    "@types/node": "^22.13.17",
    "cpy-cli": "^5.0.0",
    "rimraf": "^6.0.1",
    "terser": "^5.39.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "@types/node": ">=22.13.11"
  }
}
