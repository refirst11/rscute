{
  "name": "rscute",
  "version": "0.2.8",
  "description": "Rust Execute (rscute): Run TypeScript (ESMãƒ»CJS) in memory with SWC",
  "keywords": [
    "cli",
    "runtime",
    "node",
    "npx",
    "pnpm exec",
    "cjs",
    "commonjs",
    "esm",
    "ecmascript",
    "typescript",
    "typescript runner"
  ],
  "author": "Refirst",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/refirst11/rscute.git"
  },
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./execute": {
      "import": "./dist/execute.js",
      "require": "./dist/execute.js",
      "types": "./dist/execute.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    },
    "./register": {
      "import": "./dist/register.js",
      "require": "./dist/register.js",
      "types": "./dist/register.d.ts"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "rscute": "./dist/cli.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rimraf dist && tsc && pnpm minify",
    "minify": "terser dist/execute.js -c -o dist/execute.js",
    "bundle": "pnpm cpy 'bundle/**' 'dist'",
    "test": "node dist/cli.js test/test.tsx && pnpm cli && pnpm register",
    "cli": "node test/cli-api-test.js",
    "register": "node test/register-api-test.js"
  },
  "dependencies": {
    "@swc/core": "^1.13.5"
  },
  "devDependencies": {
    "@types/node": "^22.18.8",
    "rimraf": "^6.0.1",
    "terser": "^5.44.0",
    "typescript": "^5.9.3"
  }
}
