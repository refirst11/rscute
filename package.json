{
  "name": "rscute",
  "version": "0.2.1",
  "description": "Rust Execute (rscute): Run TypeScript ESM & CJS files in VM with SWC",
  "keywords": [
    "cli",
    "runtime",
    "node",
    "cjs",
    "commonjs",
    "esm",
    "typescript",
    "typescript runner"
  ],
  "author": "Refirst",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/refirst11/rscute.git"
  },
  "license": "MIT",
  "main": "dist/execute.js",
  "types": "types/execute.d.ts",
  "bin": {
    "rscute": "./dist/execute.js"
  },
  "files": [
    "dist/",
    "types/"
  ],
  "scripts": {
    "build": "rimraf dist types && npx tsc && node shebang && pnpm minify",
    "minify": "terser dist/execute.js -c -o dist/execute.js",
    "test": "node dist/execute.js test/test.tsx"
  },
  "dependencies": {
    "@swc/core": "^1.11.22"
  },
  "devDependencies": {
    "@types/node": "^22.13.17",
    "rimraf": "^6.0.1",
    "terser": "^5.39.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "@types/node": ">=22.13.11"
  }
}
